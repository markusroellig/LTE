        RowBox[{"-", "1"}]], 
       SuperscriptBox["sr", 
        RowBox[{"-", "1"}]]}]], "InlineFormula"],
     " to the Rayleigh Jeans equivalent temperature ",
     Cell[BoxData[
      SubscriptBox["T", "R"]], "InlineFormula"],
     " in K"
    }], "TableText"]},
   {
    RowBox[{"Kelvin2Erg", "[", 
     RowBox[{"f", ",", "T"}], "]"}], Cell[TextData[Cell[BoxData[Cell[TextData[{
      "converts radiation temperature T  at frequency f from K to specific \
intensity in ",
      Cell[BoxData[
       RowBox[{"erg", " ", 
        SuperscriptBox["s", 
         RowBox[{"-", "1"}]], 
        SuperscriptBox["cm", 
         RowBox[{"-", "1"}]], 
        SuperscriptBox["Hz", 
         RowBox[{"-", "1"}]], 
        SuperscriptBox["sr", 
         RowBox[{"-", "1"}]]}]], "InlineFormula"],
      " "
     }], "TableText"]]]], "TableText"]},
   {
    RowBox[{"Erg2KelvinKmsec", "[", 
     RowBox[{"f", ",", "I"}], "]"}], Cell[TextData[Cell[BoxData[Cell[TextData[{
      "converts line-integrated intensity I  at frequency f from ",
      Cell[BoxData[
       RowBox[{"erg", " ", 
        SuperscriptBox["s", 
         RowBox[{"-", "1"}]], 
        SuperscriptBox["cm", 
         RowBox[{"-", "1"}]], 
        SuperscriptBox["sr", 
         RowBox[{"-", "1"}]]}]], "InlineFormula"],
      " to the Rayleigh Jeans equivalent temperature ",
      Cell[BoxData[
       RowBox[{"\[Integral]", 
        RowBox[{
         SubscriptBox["T", "R"], 
         RowBox[{"\[DifferentialD]", "v"}]}]}]], "InlineFormula"],
      " in ",
      Cell[BoxData[
       RowBox[{"K", " ", "km", " ", 
        SuperscriptBox["s", 
         RowBox[{"-", "1"}]]}]], "InlineFormula"]
     }], "TableText"]]]], "TableText"]},
   {
    RowBox[{"KelvinKmsec2Erg", "[", 

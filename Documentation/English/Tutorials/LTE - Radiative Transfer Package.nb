Cell["Rayleigh-Jeans Approximation", "Section",
 CellID->44395982],

Cell[BoxData[GridBox[{
   {
    RowBox[{"LTERadiationTemperature", "[", 
     RowBox[{"T", ",", "f", ",", "\[CapitalDelta]f"}], "]"}], Cell["\<\
computes the Rayleigh Jeans radiation temperature of an optically thin black \
body with temperature T at frequency f and frequency width \[CapitalDelta]f.\
\>", "TableText"]},
   {
    RowBox[{"FromRayleighTemperature", "[", 
     RowBox[{
      SubscriptBox["T", "R"], ",", "f"}], "]"}], Cell[TextData[{
     "corrects a Rayleigh-Jeans radiation temperature ",
     Cell[BoxData[
      SubscriptBox["T", "R"]], "InlineFormula"],
     " at a given frequency f to the corresponding kinetic temperatures of a \
blackbody (necessary to yield the same intensity).\n"
    }], "TableText"]},
   {
    RowBox[{"RayleighJeans", "[", 
     RowBox[{"T", ",", "f"}], "]"}], Cell[TextData[{
     "computes the Rayleigh-Jeans approximation ",
     Cell[BoxData[
      FormBox[
       RowBox[{
        SubscriptBox["I", "\[Nu]"], "=", 
        RowBox[{
         FractionBox[
          RowBox[{" ", 
           RowBox[{"(", 
            RowBox[{"2", " ", 
             SuperscriptBox["f", "2"], " ", 
             SubscriptBox["k", "B"]}], ")"}]}], 
          SuperscriptBox["c", "2"]], "T"}]}], TraditionalForm]], "InlineMath"]
    }], "TableText"]},
   {
    RowBox[{"Planck", "[", 
     RowBox[{"T", ",", "f"}], "]"}], Cell[TextData[{
     "computes the Planck intensity ",
     Cell[BoxData[
      FormBox[
       RowBox[{
        RowBox[{
         SubscriptBox["B", "\[Nu]"], "(", "T", ")"}], "=", 
        FractionBox[
         RowBox[{"2", " ", "h", " ", 
          SuperscriptBox["\[Nu]", "3"]}], 
         RowBox[{
          SuperscriptBox["c", "2"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"exp", "(", 
             FractionBox[
              RowBox[{"h", " ", "\[Nu]"}], 
              RowBox[{
               SubscriptBox["k", "B"], " ", "T"}]], ")"}], "-", "1"}], 
           ")"}]}]]}], TraditionalForm]]]
    }], "TableText"]}
  }]], "DefinitionBox",
 CellID->350543778],

Cell["Radio astronomy quantities.", "Caption",
 CellID->714947509],

Cell[TextData[{
 "In radio astronomy, it is common to express observed intensities in terms \
of  Rayleigh-Jeans equivalent temperatures ",
 Cell[BoxData[
  SubscriptBox["T", "R"]], "InlineFormula"],
 ". Generally, a black body emits radiation with a specific intensity of "
}], "Text",
 CellID->230517283],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["B", "\[Nu]"], "(", 
    SubscriptBox["T", "kin"], ")"}], "=", 
   FractionBox[
    RowBox[{"2", " ", "h", " ", 
     SuperscriptBox["\[Nu]", "3"]}], 
    RowBox[{
     SuperscriptBox["c", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"exp", "(", 
        FractionBox[
         RowBox[{"h", " ", "\[Nu]"}], 
         RowBox[{"k", " ", 
          SubscriptBox["T", "kin"]}]], ")"}], "-", "1"}], ")"}]}]]}], 
  TraditionalForm]], "InlineMath"]], "DisplayMath",
 CellID->186398455],

Cell[TextData[{
 "In case of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"h", " ", "\[Nu]"}], "\[LessLess]", 
    RowBox[{"k", " ", 
     SubscriptBox["T", "kin"]}]}], TraditionalForm]], "InlineMath"],
 " this converges to the Rayleigh-Jeans law"
}], "Text",
 CellID->133520680],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["I", "\[Nu]"], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SubscriptBox["T", "kin"], " ", 
      RowBox[{"(", 
       RowBox[{"2", " ", "k", " ", 
        SuperscriptBox["\[Nu]", "2"]}], ")"}]}], 
     SuperscriptBox["c", "2"]], "\[GreaterEqual]", 
    RowBox[{
     SubscriptBox["B", "\[Nu]"], "(", 
     SubscriptBox["T", "kin"], ")"}]}]}], 
  TraditionalForm]], "InlineMath"]], "DisplayMath",
 CellID->663880281],


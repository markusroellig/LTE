Cell["Assuming a Gaussian doppler line profile we get:", "Text",
 CellID->774230133],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Tau]", "(", "v", ")"}], "=", 
   RowBox[{
    SubscriptBox["\[Tau]", "0"], " ", 
    RowBox[{"exp", "(", 
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"v", "-", 
          SubscriptBox["v", "0"]}], ")"}], "2"], 
       RowBox[{"2", 
        SuperscriptBox["\[Sigma]", "2"]}]]}], ")"}]}]}], 
  TraditionalForm]], "InlineFormula"]], "DisplayMath",
 CellID->588255467],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  SubscriptBox["v", "0"]], "InlineFormula"],
 " is the velocity in the local standard of rest ",
 Cell[BoxData[
  RowBox[{"(", 
   SubscriptBox["v", "LSR"], ")"}]], "InlineFormula"],
 ". ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[Sigma]", 
    RowBox[{"(", 
     RowBox[{"km", "/", "s"}], ")"}]}], "=", 
   RowBox[{"\[CapitalDelta]v", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"km", "/", "s"}], ")"}], "/", 
     RowBox[{"(", 
      RowBox[{"2", 
       SqrtBox[
        RowBox[{"2", " ", "ln", 
         RowBox[{"(", "2", ")"}]}]]}], ")"}]}]}]}]], "InlineFormula"],
 ", where ",
 Cell[BoxData["\[CapitalDelta]v"], "InlineFormula"],
 " is the full-width at half maximum."
}], "Text",
 CellID->230318280]
}, Open  ]]
